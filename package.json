{
  "name": "kids-server-launch-projects",
  "publisher": "BrucePi", 
  "displayName": "kids-server-launch-projects",
  "description": "auto launch kids .net projects in vscode",
  "icon": "images/icon.png",
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kids-server-dotnet-launcher.launchProjects",
        "title": "Kids Launch .NET Services",
        "key": "ctrl+f10"
      },
      {
        "command": "kids-server-node-launcher.launchProjects",
        "title": "Kids Launch Node Services",
        "key": "ctrl+f11"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xgtz421/kids-server-launch-projects.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
